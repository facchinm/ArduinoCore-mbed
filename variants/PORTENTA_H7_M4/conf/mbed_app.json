{
  "target_overrides": {
    "*": {
      "target.printf_lib": "std",
      "platform.stdio-buffered-serial": true,
      "target.usb_speed": "USE_USB_OTG_FS",
      "platform.stdio-baud-rate": 115200,
      "platform.default-serial-baud-rate": 115200,
      "platform.callback-nontrivial": true,
      "platform.all-stats-enabled": true,
      "rtos.main-thread-stack-size": 32768,
      "cordio.max-connections": 5,
      "target.xip-enable": true,
      "target.mbed_app_start": "0x60000000",
      "target.mbed_rom_start": "0x60000000",
      "target.mbed_ram_start": "0x60040000",
      "target.mbed_app_size": "0x40000",
      "target.mbed_rom_size": "0x40000",
      "target.mbed_ram_size": "0x40000",
      "target.device_has_remove": [ "EMAC" ],
      "target.macros_add": [
        "METAL_INTERNAL",
        "VIRTIO_SLAVE_ONLY",
        "NO_ATOMIC_64_SUPPORT",
        "METAL_MAX_DEVICE_REGIONS=2",
        "RPMSG_BUFFER_SIZE=100"
      ]
    }
  }
}